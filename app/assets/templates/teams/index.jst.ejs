<% if(filterAttr === false) { %>
  <div class="jumbotron jumbotron-teams">
    <span id="jumbo-text-1"><h1>FIND YOUR TEAM</h1></span>
    <span id="jumbo-text-2"><h1>START YOUR RUN</h1></span>
  </div>
  <div class="arrow-box">
    <input type="text" class="search" name="search" placeholder="San Francisco...">
    <span class="glyphicon glyphicon-search"></span>
    Search by location or browse all teams below
  </div>

<% } else { %>
  <h1 class="my-teams-header text-center">My Teams</h1>
  <hr class="expand-in">
<% } %>

<div class="team-index-items row"></div>
